#!/bin/sh
if [ -f `dirname $0`/environment-load ]; then . `dirname $0`/environment-load; fi

TIMESTAMP=`date +%s`

if [ "$@" ]
then
	for TARGET in $@
	do
		echo $TARGET
	done
else
	cat
fi | sort | uniq | while read TARGET
do
	nmap -vvv $NMAP_UDP_SCAN_METHOD -F -oA "fast_udp_scan_$TARGET_$TIMESTAMP" $TARGET
done
